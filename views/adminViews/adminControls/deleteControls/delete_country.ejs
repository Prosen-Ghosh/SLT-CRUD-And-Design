<!--Delete Country Model-->

          <div id='deletecountry-<%- country_id %>' class="modal fade bs-example-modal-sm"> <!--Model Start-->
            <div class="modal-dialog modal-sm">
              <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title text-warning" id="deleteTitle-<%- country_id %>">Delete Country</h4>
                </div>
                <div class="modal-body">
                  <form method="POST" name="delete-<%- country_id %>" id="delete-<%- country_id %>" enctype="application/x-www-form-urlencoded">
                    <input type="hidden" name="_method" value="delete"/>
                    <div class="form-group">
                      <input type="hidden" class="form-control" id="country_id" name='country_id' value='<%- country_id %>' />
                    </div>
                    <div class="form-group">
                        <div class="alert alert-danger" role="alert">Are You Sure? <br/><span>It Will Delete Forever</span></div>
                    </div>
                  </form>
                </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="submit" form="delete-<%- country_id %>" class="btn btn-danger ">Delete</button>
              </div>
              </div>
            </div>
          </div><!--Model End-->
