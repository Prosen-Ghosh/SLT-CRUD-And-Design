<div> <!--Model Div Start-->
  <!-- Add New Country -->
  <div>
    <button class="btn btn-info active" id='countryModelBtn'>Add New Country</button>
  </div>
  <br/>
  <div id='myModel'class="modal fade">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">Add New Country</h4>
        </div>
        <div class="modal-body">
          <form method="POST" name="form1" id="form1">
            <div class="form-group">
                <label for="country">Country Name:</label>
                <input type="text" class="form-control" id="country" name='country' placeholder="Ex: Bangladesh" autofocus>
            </div>

          </form>
        </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-warning ">Reset</button>
        <button type="submit" form="form1" class="btn btn-success ">Submit</button>
      </div>
      </div>
    </div>
  </div>
</div> <!--Model Div End-->
