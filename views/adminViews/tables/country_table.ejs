<div><!--Table Start-->
  <div class="row col-md-9 custyle">
    <table class="table table-striped table-borderd table-hover" id='countryTable'>
      <thead>
          <tr>
            <th>Serial No</th>
            <th>ID</th>
            <th>Name</th>
            <th class="text-center">Action</th>
          </tr>
      </thead>
            <% for(var i = 0; i < databaseData.length; i++){ %>
              <tr>
                  <td><%- i+1 %></td>
                  <td><%- databaseData[i].country_id %></td>
                  <td><%- databaseData[i].country_name %></td>
                  <td class="text-center"><a class='btn btn-info btn-xs' href="#"><span class="glyphicon glyphicon-edit"></span> Edit</a>
                  <a href="#" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span> Delete</a></td>
              </tr>
            <% } %>
      </table>
    </div>
</div><!--Table Start-->
