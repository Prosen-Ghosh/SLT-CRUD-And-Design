<div><!--Table Start-->
  <div class="row col-md-9 custyle ">
    <table class="table table-striped custab tableSearch">
      <thead>
          <tr>
              <th>Content Id</th>
              <th>Speech Type</th>
              <th>Hint</th>
              <th class="text-center">Action</th>
          </tr>
      </thead>
        <% for(var i = 0; i < databaseData.length; i++){ %>
              <tr>
                  <td><%- databaseData[i].content_id %></td>
                  <td><%- databaseData[i].speech_type %></td>
                  <td><%- databaseData[i].hint %></td>
                  <td><button class="btn btn-info btn-xs glyphicon glyphicon-edit" id='editBtn<%- databaseData[i].speech_id %>' onclick="openEditModel(<%- databaseData[i].speech_id %>);">Edit</button>

                  <%- include('./../adminControls/editControls/edit_speech',{
                     speech_id : databaseData[i].speech_id,
                     content_id : databaseData[i].content_id,
                     speech_type : databaseData[i].speech_type,
                     hint : databaseData[i].hint
                   })%>

                  <button class="btn btn-danger btn-xs glyphicon glyphicon-remove" id='deleteBtn<%- databaseData[i].speech_id %>' onclick="openDeleteModel(<%- databaseData[i].speech_id %>);">Delete</button>

                  <%- include('./../adminControls/deleteControls/delete_speech',{
                     speech_id : databaseData[i].speech_id
                   })%>
                 </td>
              </tr>
         <% } %>
      </table>
    </div>
</div><!--Table Start-->
